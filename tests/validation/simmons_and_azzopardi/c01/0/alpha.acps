FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha.acps;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0225;

boundaryField
{
    outlet
    {
        type             inletOutlet;
        inletValue       uniform 0;
    }
    walls
    {
        type            zeroGradient;
    }
    oilInlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    waterInlet
    {
        type            fixedValue;
        value           uniform 0;
    }
}


