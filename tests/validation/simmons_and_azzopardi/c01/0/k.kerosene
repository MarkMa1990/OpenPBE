FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k.kerosene;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.34e-04; 

boundaryField
{
    outlet
    {
        type            inletOutlet;
        phi             phi.kerosene;
        inletValue      uniform 1.34e-04;
    }
    walls
    {
        type            kqRWallFunction;
	value		uniform 1e-10;
    }
    oilInlet
    {
        type            fixedValue;
        value           uniform 1.34e-04;
    }
    waterInlet
    {
        type            fixedValue;
        value           uniform 1.34e-04;
    }
}


